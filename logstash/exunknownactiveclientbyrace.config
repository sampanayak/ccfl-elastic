input {
	file {
		path => "/home/snayak/ccfl-data/subsidy-data/ExUnknown-Activeclientbyrace.csv"
		start_position => "beginning"
		ignore_older => 1
		sincedb_path => "/dev/null"
	}
}

filter {
	csv {
		separator => ","
		columns => ["Race", "# Client", "Date"]
	}
	mutate {
		convert => {"#Client" => "integer"}
	}
	date {
		match => ["Date", "MM/yyyy"]
		target => "Date"
	}
}

output {
	elasticsearch {
		hosts => "http://localhost:9200"
		index => "subsidy-exunknownactiveclientbyrace"
	}
stdout {}
}